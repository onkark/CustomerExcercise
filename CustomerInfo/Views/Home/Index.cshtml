    <div class="user-details" data-ng-show="selectedUser">
        <div class="well well-large">
            <form class="ng-pristine ng-valid" id="customerInfo">

                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="firstName">First Name<span class="requiredField">*</span></label>
                        <input name="firstName" class="ng-pristine ng-valid" id="firstName" type="text" data-ng-model="selectedUser.FirstName">
                    </div>
                    <div class="col-xs-6 col-sm-3">
                        <label for="lastName">Last Name</label>
                        <input name="lastName" class="ng-pristine ng-valid" id="lastName" type="text" data-ng-model="selectedUser.LastName">
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="gender">Gender</label>
                        <input name="gender" class="ng-pristine ng-valid" id="male" type="radio" value="M" data-ng-model="selectedUser.Gender"><span class="gender-text">Male</span>
                        <input name="gender" class="ng-pristine ng-valid" id="female" type="radio" value="F" data-ng-model="selectedUser.Gender"><span class="gender-text">Female</span><br>
                    </div>
                    <div class="col-xs-6 col-sm-3">
                        <label for="title">Title</label>
                        <select name="title" class="ng-pristine ng-valid" id="title" data-ng-model="selectedUser.Title" data-ng-options="title.id as title.title for title in selectedTitles"><option value="0">Ms.</option><option value="1">Mrs.</option><option value="2">Dr.</option></select>
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="mobile">Mobile</label>
                        <input name="mobile" class="ng-pristine ng-valid" id="mobile" type="tel" data-ng-model="selectedUser.Mobile">
                    </div>
                    <div class="col-xs-6 col-sm-3">
                        <label for="email">Email</label>
                        <input name="email" class="ng-pristine ng-valid ng-valid-email" id="email" type="email" data-ng-model="selectedUser.Email">
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="country">Country</label>
                        <select name="country" class="ng-pristine ng-valid" id="country" data-ng-model="selectedUser.Country" data-ng-options="country.id as country.name for country in countryList"><option value="0">USA</option></select>
                    </div>
                    <div class="col-xs-6 col-sm-3">
                        <label for="state">State</label>
                        <select name="state" class="ng-pristine ng-valid" id="state" data-ng-model="selectedUser.State" data-ng-options="s.id as s.name for s in countryList[0].states" data-ng-change="clearCityAndZip();" data-ng-disabled="!selectedUser.Country"><option value="0">Alabama</option><option value="1">Alaska</option><option value="2">New Jersey</option><option value="3">New Carolina</option><option value="4">New York</option><option value="5">Texas</option></select>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="city">City</label>
                        <select name="city" class="ng-pristine ng-valid" id="city" data-ng-model="selectedUser.City" data-ng-options="c.id as c.name for c in selectedState.cities"><option selected="selected" value="">Select City</option><option value="0">Charlotte</option><option value="1">Religh</option><option value="2">Wilmington</option></select>
                    </div>
                    <div class="col-xs-6 col-sm-3">
                        <label for="zip">Zip</label>
                        <input name="zip" class="ng-pristine ng-valid" id="zip" type="text" data-ng-model="selectedUser.Zip"><br>
                    </div>
                </div>


                <div class="form-group row">
                    <div class="col-xs-6 col-sm-3">
                        <label for="ssn">SSN</label>
                        <input name="ssn" class="ng-pristine ng-valid" id="ssn" type="text" maxlength="9" data-ng-model="selectedUser.SSN"><br>
                    </div>
                </div>

                <div class="form-group">
                    
                    <input class="btn" id="btnSubmit" type="button" value="UPDATE" data-ng-click="updateUser(selectedUser)">
                </div>


            </form>
        </div>
    </div>
</div>

<script class="ng-scope">
    jQuery(function ($) {
        $("#ssn").mask("999-99-9999");
    });
</script>
</div>